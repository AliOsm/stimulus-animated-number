{
  "name": "stimulus-animated-number",
  "version": "4.1.0",
  "description": "A Stimulus controller that animates a numerical value by counting to it.",
  "keywords": [
    "stimulus",
    "stimulusjs",
    "stimulus controller",
    "animated number"
  ],
  "repository": "git@github.com:stimulus-components/stimulus-animated-number.git",
  "bugs": {
    "url": "https://github.com/stimulus-components/stimulus-animated-number/issues"
  },
  "author": "Guillaume Briday <guillaumebriday@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/stimulus-components/stimulus-animated-number",
  "private": false,
  "main": "dist/stimulus-animated-number.umd.js",
  "module": "dist/stimulus-animated-number.mjs",
  "scripts": {
    "format": "prettier-standard  '**/*.{ts,css,html}' --format",
    "lint": "prettier-standard  '**/*.{ts,css,html}' --lint",
    "lintfix": "yarn format && yarn lint",
    "dev": "vite",
    "prod": "vite build --mode netlify",
    "build": "tsc --noEmit && vite build",
    "version": "yarn build",
    "np": "np --no-2fa --no-test"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/plugin-syntax-class-properties": "7.12.13",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@hotwired/stimulus": "^3.2.1",
    "np": "^7.6.2",
    "prettier-standard": "16.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.1"
  },
  "peerDependencies": {
    "@hotwired/stimulus": "^3.2.1"
  }
}
